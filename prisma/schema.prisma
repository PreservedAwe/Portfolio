generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model projects {
  id           Int      @id @default(autoincrement())
  github_link  String
  description  String
  name         String
}

model sessions {
  client_id    String   @id
  createdAt    DateTime @default(now())
}