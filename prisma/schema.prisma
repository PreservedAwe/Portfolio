generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model project {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  github       String
  language     String
  project_name String
}

model hobby {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  type         String
  bio          String
  name         String
}
