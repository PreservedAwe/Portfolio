generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model projects {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  github_link  String
  description  String
  name         String
}

model sessions {
  client_id    String @id @map("_id")
  createdAt    DateTime @default(now())
}