generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model projects {
  id           String   @id @default(uuid())
  github_link  String
  description  String
  name         String
}

model sessions {
  client_id    String   @id
  createdAt    DateTime @default(now())
}

model visitors {
  visitor_id   String   @id
  confidence   String
  os           String
  timezone     String
  country      String
  city         String
  isp          String
  timestamp    String
}