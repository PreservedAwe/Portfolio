generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model projects {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  github_link  String
  description  String
  name         String
}

model hobbies {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  type         String
  bio          String
  name         String
}
